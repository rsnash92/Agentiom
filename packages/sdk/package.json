{
  "name": "@agentiom/sdk",
  "version": "0.1.0",
  "description": "Build stateful AI agents that sleep between tasks",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "agent",
    "ai",
    "stateful",
    "serverless",
    "slack",
    "discord",
    "telegram",
    "webhook"
  ],
  "author": "Agentiom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentiom/agentiom.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://agentiom.dev"
}
