{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd ../.. && turbo build --filter=@agentiom/api",
  "installCommand": "cd ../.. && bun install",
  "framework": null,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ]
}
